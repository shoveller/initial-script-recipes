# aws 관련 환경변수
AWS_ACCOUNT_ID=
AWS_DEFAULT_REGION=ap-northeast-2

# Cloudflare 관련 (DNS 업데이트 시 필요)
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_ACCOUNT_ID=

### DNS 업데이트 관련 환경변수
# 도메인 설정 (선택사항 - 없으면 DNS 업데이트 건너뜀)
DOMAIN=example.com
# 선택사항 - 없으면 메인 도메인 사용
SUBDOMAIN=

# DNS 레코드 설정
RECORD_TYPE=CNAME
# 람다함수 또는 workers 의 cname, 자동 업데이트됨
RECORD_VALUE=
TTL=300